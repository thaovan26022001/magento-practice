<block class="Magento\Framework\View\Element\Template" name="myblock" template="Magento_Theme::myview.phtml" ></block>
</container>


<referenceBlock name="report.bugs" remove="true"/>
<move element="copyright" destination="before.body.end"/>
</body>
</page>